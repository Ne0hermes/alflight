# Configuration OpenAIP
# Obtenez votre clé API sur https://www.openaip.net/
OPENAIP_API_KEY=your_openaip_api_key_here

# Port du serveur proxy OpenAIP
OPENAIP_PROXY_PORT=3001

# Configuration du serveur de développement
VITE_OPENAIP_PROXY_URL=http://localhost:3001

# ========================================
# Configuration IA pour l'analyse des tableaux de performances
# ========================================

# Option 1: OpenAI GPT-4 Vision (recommandé pour la meilleure précision)
# Obtenez votre clé API sur: https://platform.openai.com/api-keys
# VITE_OPENAI_API_KEY=your_openai_api_key_here
# VITE_AI_API_ENDPOINT=https://api.openai.com/v1/chat/completions

# Option 2: Claude Vision API (alternative)
# VITE_CLAUDE_API_KEY=your_claude_api_key_here
# VITE_AI_API_ENDPOINT=https://api.anthropic.com/v1/messages

# Option 3: Modèle IA local (gratuit, mais moins précis)
# VITE_AI_API_ENDPOINT=http://localhost:11434/api/generate
# Nécessite Ollama avec un modèle vision comme llava

# ========================================
# Configuration API météo
# ========================================

# CheckWX API (recommandé pour la météo aviation)
# Obtenez votre clé API gratuite sur: https://www.checkwx.com/api
REACT_APP_WEATHER_API_KEY=your_checkwx_api_key
REACT_APP_WEATHER_API_PROVIDER=checkwx

# Alternative: OpenWeatherMap
# REACT_APP_WEATHER_API_PROVIDER=openweathermap
# REACT_APP_WEATHER_API_KEY=your_openweathermap_api_key

# Alternative: AVWX API
# REACT_APP_WEATHER_API_PROVIDER=avwx
# REACT_APP_WEATHER_API_KEY=your_avwx_api_key